{
  "name": "tailwind-demo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "@headlessui/react": "^2.1.2",
    "@heroicons/react": "^1.0.6",
    "@mui/icons-material": "^5.16.7",
    "@mui/material": "^5.16.7",
    "@radix-ui/react-context": "^1.1.2",
    "@radix-ui/react-direction": "^1.1.1",
    "@radix-ui/react-primitive": "^2.1.3",
    "@radix-ui/react-roving-focus": "^1.1.10",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@rive-app/canvas": "^2.27.3",
    "@shadcn/ui": "^0.0.4",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "electron-squirrel-startup": "^1.0.1",
    "ffmpeg": "^0.0.4",
    "jszip": "^3.10.1",
    "lucide-react": "^0.511.0",
    "next-themes": "^0.4.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1",
    "tailwind-merge": "^3.3.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "electron-forge start",
    "dev": "cross-env NODE_ENV=development vite",
    "build": "vite build",
    "build:main": "vite build -c vite.main.config.mjs",
    "build:preload": "vite build -c vite.preload.config.mjs",
    "build:renderer": "vite build -c vite.renderer.config.mjs",
    "build:all": "npm run build:main && npm run build:preload && npm run build:renderer",
    "preview": "vite preview",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "test": "react-scripts test"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron-forge/plugin-vite": "^7.2.0",
    "@electron/fuses": "^1.8.0",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@tailwindcss/forms": "^0.5.10",
    "@vitejs/plugin-react": "^4.5.2",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "electron": "^28.0.0",
    "postcss": "^8.5.5",
    "postcss-import": "^16.1.0",
    "tailwindcss": "^4.1.10",
    "tailwindcss-animate": "^1.0.7",
    "vite": "^4.5.2",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6"
  },
  "main": "src/main.js",
  "build": {
    "appId": "com.tailwind.demo",
    "productName": "TailwindDemo",
    "files": [
      "build/**/*",
      "main.js",
      "preload.js"
    ],
    "directories": {
      "buildResources": "public",
      "output": "dist"
    },
    "asar": false,
    "compression": "store",
    "win": {
      "target": "portable"
    },
    "npmRebuild": false,
    "buildDependenciesFromSource": false
  }
}
