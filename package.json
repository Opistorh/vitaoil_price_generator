{
  "name": "vitaoil_price_generator",
  "version": "0.1.0",
  "private": true,
  "bin": "server.js",
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "@headlessui/react": "^2.1.2",
    "@heroicons/react": "^1.0.6",
    "@mui/icons-material": "^5.16.7",
    "@mui/material": "^5.16.7",
    "@radix-ui/react-context": "^1.1.2",
    "@radix-ui/react-direction": "^1.1.1",
    "@radix-ui/react-primitive": "^2.1.3",
    "@radix-ui/react-roving-focus": "^1.1.10",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@rive-app/canvas": "^2.27.3",
    "@shadcn/ui": "^0.0.4",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "express": "^5.1.0",
    "ffmpeg": "^0.0.4",
    "http-server": "^14.1.1",
    "jszip": "^3.10.1",
    "lucide-react": "^0.511.0",
    "next-themes": "^0.4.6",
    "path-to-regexp": "^8.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1",
    "tailwind-merge": "^3.3.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:win": "pkg . --targets node18-win-x64 --output build/server-win.exe",
    "pkg:mac": "npm run build && pkg . --targets node18-macos-arm64 --output dist/vitaoil_price_generator-macos-arm64",
    "pkg:win": "npm run build && pkg . --targets node18-win-x64 --output dist/vitaoil_price_generator-win.exe",
    "pkg:all": "npm run build && pkg . --targets node18-macos-arm64,node18-win-x64 --out-path dist",
    "build:installer": "pkg installer.js --targets node18-macos-arm64 -o vitaoil-installer && codesign --force --deep --sign - ./vitaoil-installer",
    "dist:quick": "tar -czf vitaoil-quick.tar.gz quick-start.command README_INSTALL.md",
    "dist:zip": "zip -j vitaoil-quick.zip quick-start.command README_INSTALL.md"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^22.15.21",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.37",
    "autoprefixer": "^10.4.21",
    "pkg": "^5.8.1",
    "postcss": "^8.4.41",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^3.4.9",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^4.9.5"
  },
  "pkg": {
    "scripts": [
      "server.js"
    ],
    "assets": [
      "build/index.html",
      "build/**",
      "build/**/*",
      "public",
      "public/**",
      "public/**/*"
    ]
  }
}
